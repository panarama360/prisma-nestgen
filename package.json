{
  "name": "generate-nest-application",
  "version": "1.0.0",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "camelcase": "^6.2.0",
    "pluralize": "^8.0.0",
    "prisma-schema-dsl": "^1.1.5"
  },
  "devDependencies": {
    "@types/camelcase": "^5.2.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/pluralize": "^0.0.29",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  }
}
